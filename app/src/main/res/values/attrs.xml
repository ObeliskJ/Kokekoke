<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GoogleProgressBar">
        <attr name="type" format="enum">
            <enum name="folding_circles" value="0" />
            <enum name="google_music_dices" value="1" />
            <enum name="nexus_rotation_cross" value="2" />
            <enum name="chrome_floating_circles" value="3" />
        </attr>
        <attr name="colors" format="reference"/>
    </declare-styleable>

</resources>